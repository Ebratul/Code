#include <bits/stdc++.h>
#define int long long int
#define endl '\n'
using namespace std;

const int N = 1e5 + 5;
const int mod = 1e9 + 7;
const int mn = -1;
const int mx = 1e18+7;

int32_t main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int n,m;
    cin>>n>>m;
    int adj[n+1][m+1];
    for(int i=0;i<m;i++)
    {
      int u,v;
      cin>>u>>v;
      adj[u][v]=1;
      adj[v][u]=1;
    }

    
    return 0;
}
